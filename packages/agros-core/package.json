        "@types/jest": "^28.1.1",
        "@types/lodash": "^4.14.182",
        "@types/node": "^15.12.2",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "customize-cra": "^1.0.0",
        "react-app-rewired": "^2.2.1",
        "react-dev-utils": "^12.0.1",
        "react-error-boundary": "^3.1.4",
        "react-scripts": "^5.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.5",
        "web-vitals": "^2.1.4"
    }
