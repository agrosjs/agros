{
    "name": "@agros/core",
    "version": "0.3.1",
    "description": "Core package for Agros.js",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "install:example": "cd examples && npm i",
        "example": "node ./scripts/example.js",
        "lint": "eslint .",
        "build": "rimraf ./lib && tsc --build tsconfig.json",
        "watch": "tsc --watch --project tsconfig.json",
        "checksum": "node ./scripts/checksum.js",
        "publish:main": "node ./scripts/publish.js",
        "pretest": "npm run lint",
        "test": "exit 0"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lenconda/agros.git"
    },
    "author": "Lenconda <i@lenconda.top>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lenconda/agros/issues"
    },
    "files": [
        "lib",
        "bin",
        "LICENSE",
        "README.md",
        "package.json",
        "package-lock.json"
    ],
    "homepage": "https://github.com/lenconda/agros#readme",
    "dependencies": {
        "immutable": "^4.1.0",
        "is-promise": "^4.0.0",
        "lodash": "^4.17.21",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-router-dom": "^6.3.0",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
