import {
    Module,
    RouterModule,
} from '@agros/app';
import { AppComponent } from '@/app.component';
import { AppService } from '@/app.service';
import { AppInterceptor } from '@/app.interceptor';

@Module({
    imports: [
        RouterModule.forRoot({
            routes: [
                {
                    path: '',
                    useComponentClass: AppComponent,
                },
            ],
        }),
    ],
    providers: [
        AppInterceptor,
        AppService,
    ],
    components: [
        AppComponent,
    ],
    exports: [
        AppComponent,
        AppInterceptor,
        AppService,
    ],
})
export class AppModule {}
