{
    "properties": {
        "name": {
            "type": "string",
            "message": "The name of the service",
            "cliType": "argument"
        },
        "moduleName": {
            "type": "string",
            "message": "The name of module who manages this service",
            "default": "$context$name",
            "cliType": "argument"
        },
        "noExport": {
            "type": "confirm",
            "message": "Export this service from module",
            "default": false,
            "cliType": "option"
        }
    },
    "required": [
        "name"
    ]
}
