{
    "alias": "s",
    "properties": {
        "name": {
            "type": "input",
            "message": "The name of the service",
            "cliType": "argument"
        },
        "moduleName": {
            "type": "input",
            "message": "The name of module who manages this service",
            "default": "$context$name",
            "cliType": "argument"
        },
        "skipExport": {
            "type": "confirm",
            "message": "Skip exporting this service from module",
            "default": false,
            "cliType": "option"
        }
    },
    "required": [
        "name"
    ]
}
